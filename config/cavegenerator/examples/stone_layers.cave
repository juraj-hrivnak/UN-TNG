 #               ### stone_layers.cave Info ###                #
 # ########################################################### #
 #   This preset is designed to demonstrate the use of the     #
 # included StoneLayer objects. Make sure to list these in     #
 # from bottom to top.                                         #
 # ----------------------------------------------------------- #
{
  $VANILLA: "ALL" # Default tunnels, ravines, etc.
  enabled: false
  stoneLayers: [
    {
      state: granite
      height: [ 0, 12 ]
    }
    {
      state: andesite
      height: [ 12, 23 ]
    }
    {
      state: stone
      height: [ 23, 44 ]
    }
    {
      state: diorite
      height: [ 44, 60 ]
    }
  ]
}